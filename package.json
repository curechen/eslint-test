{
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-diff": "^2.0.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "eslint"
    ]
  }
}
