{
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx",
    "prepare": "npx husky install && npx husky add .husky/pre-commit 'npx lint-staged'"
  },
  "dependencies": {
    "@qnpm/eslint-config-qunar-node": "^1.0.5",
    "@qnpm/eslint-config-qunar-react": "^1.0.5",
    "@qnpm/eslint-config-qunar-rn": "^1.0.5",
    "eslint-config-qunar-rn": "^1.1.25"
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "eslint-plugin-diff": "1.0.15",
    "@qnpm/eslint-config-qunar-base": "latest"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
