{
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx"
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "eslint-plugin-diff": "^2.0.2",
    "@qnpm/eslint-config-qunar-base": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
